{
  "name": "umi-plugin-arms",
  "version": "1.0.0-beta.1",
  "description": "Alibaba Cloud Application Real-Time Monitoring Service (ARMS) for umi.",
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "npm run lint:es && npm run lint:ts",
    "lint:es": "eslint --ext .js --ext .jsx . --fix",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\" --fix",
    "prestart": "npm run build",
    "start": "umi dev",
    "docs": "npm run build && umi build",
    "test": "umi test --coverage",
    "build": "npm run build:babel",
    "build:babel": "npx babel src --out-dir lib --extensions \".ts\" --ignore \"src/pages/*\"",
    "build:less": "lessc src/global.less lib/global.css",
    "build:umi": "npm run build:babel && umi build",
    "precommit": "lint-staged"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "index.d.ts"
  ],
  "dependencies": {
    "arms.js": "^1.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/plugin-transform-typescript": "^7.4.5",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^11.11.3",
    "babel-eslint": "^9.0.0",
    "debug": "^4.1.1",
    "eslint": "^5.12.0",
    "eslint-config-ali": "^5.0.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^0.14.3",
    "jest": "^24.8.0",
    "lessc": "^1.0.2",
    "lint-staged": "^7.2.2",
    "np": "^3.0.4",
    "prettier": "^1.18.2",
    "tslint": "^5.20.0",
    "tslint-config-ali": "^0.2.4",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3333",
    "umi": "^2.9.6",
    "umi-plugin-types": "^0.3.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx,json,ts,tsx,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "license": "MIT"
}
